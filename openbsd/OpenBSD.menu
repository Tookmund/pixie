LABEL install64
	MENU LABEL Install OpenBSD amd64
	KERNEL pxechn.c32
	APPEND ::/openbsd/6.5/amd64/pxeboot

LABEL install64floppy
	MENU LABEL Install OpenBSD amd64 floppy
	KERNEL ::/memdisk
	APPEND initrd=::/openbsd/6.5/amd64/floppy65.fs

LABEL install32
	MENU LABEL Install OpenBSD i386
	KERNEL pxechn.c32
	APPEND ::/openbsd/6.5/i386/pxeboot

LABEL install32floppy
	MENU LABEL Install OpenBSD i386 floppy
	KERNEL ::/memdisk
	APPEND initrd=::/openbsd/6.5/i386/floppy65.fs
